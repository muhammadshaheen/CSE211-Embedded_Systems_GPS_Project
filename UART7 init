void UART7_Init(void){
  SYSCTL_RCGCUART_R |= 0x80;
	SYSCTL_RCGCGPIO_R |= 0x10;
	UART7_CTL_R = 0; 
	UART7_IBRD_R = 104;
	UART7_FBRD_R = 11;
	UART7_LCRH_R  |= 0x70;
	UART7_CTL_R |= 0x301;
	GPIO_PORTE_AFSEL_R |= 0x03;
	GPIO_PORTE_PCTL_R = (GPIO_PORTE_PCTL_R & ~0x000000FF) | 0x00000011;
	GPIO_PORTE_DEN_R |= 0x03;
}
